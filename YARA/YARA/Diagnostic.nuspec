<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>YARA</id>
    <version>1.0.0.0</version>
    <title>YARA</title>
    <authors>Nikola Sivkov</authors>
    <owners>Nikola Sivkov</owners>
    <licenseUrl>https://raw.githubusercontent.com/aviatrix/YARA/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/aviatrix/YARA</projectUrl>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <description>YARA - Yet Another Roslyn Analyzer</description>
    <tags>YARA, analyzers, roslyn, entityframework</tags>
  </metadata>
  <!-- The convention for analyzers is to put language agnostic dlls in analyzers\dotnet and language specific analyzers in either analyzers\dotnet\cs or analyzers\dotnet\vb -->
  <files>
    <file src="*.dll" target="analyzers\dotnet\cs" exclude="**\Microsoft.CodeAnalysis.*;**\System.Collections.Immutable.*;**\System.Reflection.Metadata.*;**\System.Composition.*" />
    <file src="tools\*.ps1" target="tools\" />
  </files>
</package>